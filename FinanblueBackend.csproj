    <UserSecretsId>bc7b8537-9b59-49ba-9f8e-b5a7ee439223</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BCrypt.Net-Next" Version="4.0.3" />
